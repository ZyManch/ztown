	start: function(e,x,y,w,h,m) {
		var r=_uDraggable;
		if(r.obj) {r.obj.event=e;r.obj.onstopdrag.apply(r.obj.par);r.obj.event=null;}
		this.x=x;
		this.y=y;
		this.w=w;
		this.h=h;
		this.m=m;
