_uMENU.prototype={
	onfocusdeactivate: function() {
		if(this.props.statical) { //unhighlight previous item
			var prev=this.hlitem,it,hl;
			if(prev>=0) { 
				it=this.elems[prev];
				if(it && it[0]!='sep') {
					hl=this.props.highlight;
					if(it[2].hl!=undefined) hl=it[2].hl;
					if(hl) {
