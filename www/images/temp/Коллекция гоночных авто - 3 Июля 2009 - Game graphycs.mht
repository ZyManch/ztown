attachTo:function(obj,al,dx,dy) { //moves window so it became attached to specified html obj or [x,y]; al - align type, 
//dx, dy - in pixels; for primary align positiv values specify distance between object and window; for secondary - how much window's edge goes outside the object's edge
    var a,pos,ow,oh,ppos,x,y,d,ww,wh,wl,wt,al,l1,i,xL,xR,yT,tB,icon,icon2;
    if(obj && obj.nodeType || (obj[0] && obj[0].nodeType)) {
	if(obj.nodeType) a=obj; else a=obj[0]; //to allow jquery object
	pos=$(a).offset();
	ow=a.offsetWidth;
	oh=a.offsetHeight;
    } else if(obj && obj.length==2) {
	pos={left:obj[0],top:obj[1]};
	ow=oh=0;
    } else return;
    d=_uWnd.getdims();
    ww=d.clientW;
    wh=d.clientH;
    wl=d.clientLeft;
    wt=d.clientTop;
    al=al || "rt";dx=dx || 0;dy=dy || 0;
    if(this.desktop) {
	d=this.desktop;
	ww=d.width;
	wh=d.height;
	wl=0;
	wt=0;
    }
    if(this.props.fixed) {wl=wt=0;}

    if(al.length==1)
        if(al.toLowerCase()=='u' || al.toLowerCase()=='d') al+='r';
            else al+='t';
    if(window._rtl) al=al.replace(/[lLrR]/g,function(l){if(l=='l')return 'r';if(l=='L')return 'R';if(l=='r')return 'l';if(l=='R')return 'L';});
    //primary alignment
    l1=al.substr(0,1);
    xR=pos.left+ow+dx; //to the right from object
    xL=pos.left-dx-this.width; //to the left from object
    yB=pos.top+oh+dy; //to the down
    yT=pos.top-dy-this.height; //to the up
    x=xR; //fallback values
    icon='l';
    y=pos.top-dy;
    icon2='t';

    if(l1=='l') {x=xL;icon='r';}
    else if(l1=='r') {x=xR;icon='l';}
    else if(l1=='L' && xL-wl<1 && wl-xL>xR-wl+this.width-ww) {x=xR;icon='l';}
    else if(l1=='R' && xR-wl+this.width>ww-3 && wl-xL<xR-wl+this.width-ww) {x=xL;icon='r';}
    else if(l1=='u') {y=yT;icon='b';}
    else if(l1=='d') {y=yB;icon='t';}
    else if(l1=='U' && yT-wt<1 && wt-yT>yB-wt+this.height-wh) {y=yB;icon='t';}
    else if(l1=='D' && yB-wt+this.height>wh-3 && wt-yT<yB-wt+this.height-wh) {y=yT;icon='b';}

    //secondary
    l1=al.substr(1,1);
    xR=pos.left+ow-this.width+dx; //align right edge
    xL=pos.left-dx; //align left edge
    yB=pos.top+oh+-this.height+dy; //bottom edge
    yT=pos.top-dy; //top edge
    if(l1=='l') {x=xL;icon2='l';}
    else if(l1=='r') {x=xR;icon2='r';}
    else if(l1=='L' && xL-wl<1 && wl-xL>xR-wl+this.width-ww) {x=xR;icon2='r';}
    else if(l1=='R' && xR-wl+this.width>ww-3 && wl-xL<xR-wl+this.width-ww) {x=xL;icon2='l';}
    else if(l1=='t') {y=yT;icon2='t';}
    else if(l1=='b') {y=yB;icon2='b';}
    else if(l1=='T' && yT-wt<1 && wt-yT>yB-wt+this.height-wh) {y=yB;icon2='b';}
    else if(l1=='B' && yB-wt+this.height>wh-3 && wt-yT<yB-wt+this.height-wh) {y=yT;icon2='t';}

    this.showattachicon(icon+icon2);
    if(x!=this.xpos || y!=this.ypos) this.moveTo(x,y);
