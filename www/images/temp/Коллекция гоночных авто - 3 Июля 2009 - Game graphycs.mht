_foreachchild: function(func,cfirst) { //foreach child executes parent.func(child) and makes recursive call (if cfirst is false) or in reverse order (if cfirst is true)
//when cfirst is false, then func() return value shows whether to stop recursion for that window
	var c;
	for(var i=0;i<this.childs.length;i++) {
		c=this.childs[i];
		if(!c || c.state.destroyed) continue;
		if(cfirst) {
			c._foreachchild(func,cfirst);
			func.call(this,c);
		} else {
			if(!func.call(this,c))
				c._foreachchild(func,cfirst);
		}
	}
},
//show attach icon in specified corner (lt - left top, br - bottom rightt etc) or hides if empty string; undef means just to reposition (when window size changes)
showattachicon:function(tp) {
    var aa=this.props.attachicon,img,ao=this.parts.attachicon,dd=this.props.attachicondelta,
        t1={'lt':'l','lb':'l','rt':'r','rb':'r','tl':'t','tr':'t','bl':'b','br':'b'},
        t2={'lt':'h','lb':'h','rt':'h','rb':'h','tl':'v','tr':'v','bl':'v','br':'v'};
    if(!this.parts.attachicon) return;
    if(arguments.length>0) {
        if(tp && aa) {
            img=typeof(aa)=='object' ? aa[tp] : aa;
            if(!img) {
                img=aa[t1[tp]];
                if(!img) img=aa[t2[tp]];
            }
        }
        if(!img || !tp) { //hide
            this.parts.attachicon.style.display='none';
            return;
        }
    } else {
        tp=this.attachicon;
        if(!tp) return; //if reposition request but wasn't enabled
    }
    if(this.attachicon!=tp) $(ao).html(img);
    if(t2[tp]=='h') {
        if(tp=='lt' || tp=='rt')
            $(ao).css({top:0,bottom:''});
        else
            $(ao).css({bottom:(-this.height)+'px',top:''});
        if(t1[tp]=='l')
            $(ao).css({left:'',right:(-dd)+'px'});
        else
            $(ao).css({right:'',left:(this.width-dd)+'px'});
    } else { //vertical
        if(tp=='tl' || tp=='bl')
            $(ao).css({right:'',left:0+'px'});
        else
            $(ao).css({left:'',right:(-this.width)+'px'});
        if(t1[tp]=='t')
            $(ao).css({bottom:(-dd)+'px',top:''});
        else
            $(ao).css({top:(this.height-dd)+'px',bottom:''});
    }
    if(!this.attachicon) this.parts.attachicon.style.display='';
    this.attachicon=tp;
},
